    >;
};

        layer_1 {
            bindings = <
&none  &none  &kp KP_N7  &kp KP_N8  &kp KP_N9  &none          &kp LS(TAB)  &kp HASH  &kp AT  &kp TAB    &none      &none
&none  &none  &kp KP_N4  &kp KP_N5  &kp KP_N6  &none          &kp LEFT     &kp DOWN  &kp UP  &kp RIGHT  &kp UNDER  &none
&none  &none  &kp KP_N1  &kp KP_N2  &kp KP_N3  &none          &kp GRAVE    &kp LT    &kp GT  &none      &none      &none
                         &none      &kp KP_N0  &kp LG(RET)    &none        &none     &none
            >;
